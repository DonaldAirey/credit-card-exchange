<?xml version="1.0" encoding="utf-8"?>
<script name="CommissionType">
  <client name="DataModelClient" type="DataModelClient, FluidTrade.ClientDataModel" endpoint="TcpDataModelEndpoint" />
  <transaction>
    <method name="CreateCommissionTypeEx" client="DataModelClient">
      <parameter name="configurationId" value="Default" />
      <parameter name="description" value="Undefined commission type." />
      <parameter name="externalId0" value="EMPTY" />
			<parameter name="name" value="Empty" />
			<parameter name="commissionTypeCode" value="Empty" />
    </method>
    <method name="CreateCommissionTypeEx" client="DataModelClient">
      <parameter name="configurationId" value="Default" />
      <parameter name="description" value="The commission is applied as a flat fee." />
      <parameter name="externalId0" value="FEE" />
			<parameter name="name" value="Fee" />
			<parameter name="commissionTypeCode" value="Fee" />
    </method>
    <method name="CreateCommissionTypeEx" client="DataModelClient">
      <parameter name="configurationId" value="Default" />
      <parameter name="description" value="The commission is applied as a percent." />
      <parameter name="externalId0" value="PERCENT" />
			<parameter name="name" value="Percent" />
			<parameter name="commissionTypeCode" value="Percent" />
    </method>
		<method name="CreateCommissionTypeEx" client="DataModelClient">
			<parameter name="configurationId" value="Default" />
			<parameter name="description" value="The commission is applied in basis points." />
			<parameter name="externalId0" value="BASISPOINT" />
			<parameter name="name" value="BasisPoint" />
			<parameter name="commissionTypeCode" value="BasisPoint" />
		</method>
	</transaction>
</script>