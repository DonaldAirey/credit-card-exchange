<UserControl x:Class="FluidTrade.Guardian.TabMatch"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:guardian="http://schemas.fluidtrade.com/winfx/2006/xaml/guardian"
    Height="300" Width="300">
    <Grid>
      <Grid.RowDefinitions>
        <RowDefinition Height="*" />
        <RowDefinition Height="150" />
      </Grid.RowDefinitions>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>
            <guardian:ReportDebtHolderMatch Grid.Column="0" Grid.Row="0" />
            <guardian:DebtHolderNegotiationConsole Grid.Column="1" Grid.Row="1" />
        </Grid>
    </Grid>
</UserControl>
